using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace IMS.Items
{
    internal class SemiFinishGood : IProduct 
    {
        string _id = string.Empty;
        public string ID
        {
            get
            {
                return _id;
            }
        }

        public string ItemId
        {
            get;
            set;
        }

        public string Name
        {
            get;
            set;
        }

        public string GroupId
        {
            get;
            set;
        }

        public string UnitId
        {
            get;
            set;
        }

        public string DrawingNo
        {
            get;
            set;
        }

        public string ModelNo
        {
            get;
            set;
        }

        public int MinQty
        {
            get;
            set;
        }

        public string Notes
        {
            get;
            set;
        }

        public int ExpirationPeriod
        {
            get;
            set;
        }

        public void SetId(string id)
        {
            _id = id;
        }
    }
}
