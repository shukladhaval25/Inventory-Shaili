    Public strcheck As String
    Dim strCuid As String
    Dim strGroupID As String
    Private Sub FrmCustomer_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load


        FillLookup("Select GroupName,Groupid from GroupMaster where GRoupType='C'", "GroupMaster", "GroupName", "GroupID", lkupCustGroup)

        If strcheck = "Add" Then
            txtCustId.Tag = GetMaxNo("Cid", "CustomerMaster", "CS", 10)
        ElseIf strcheck = "Edit" Then
            txtCustId.Tag = ModMain.strgridid

            cmd = New OleDb.OleDbCommand
            cmd.CommandText = "select * from CustomerMaster where Cid='" & ModMain.strgridid & "'"
            cmd.Connection = conn
            dr = cmd.ExecuteReader()
            dr.Read()
            LoadCustomer()
            dr.Close()
            cmd.Dispose()
            strCuid = txtCustId.Text
        End If

    End Sub
